<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.zhihu">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.example.administrator.zhihu.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="This is FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.ArticleActivity"
            android:label="Thi is ArticleActivity"/>
        <activity
            android:name=".activity.ContentActivity"
            android:label="content activity"/>

    </application>

</manifest>
